# Terraform variables template for multi-cloud infrastructure
# Copy this file to terraform.tfvars and replace with actual values
# DO NOT commit terraform.tfvars to git - it contains sensitive information

# AWS Configuration
aws_region = "us-west-2"
aws_access_key_id = "your_aws_access_key_id"
aws_secret_access_key = "your_aws_secret_access_key"

# Cloudflare Configuration
cloudflare_api_token = "your_cloudflare_api_token"
cloudflare_zone_id = "your_cloudflare_zone_id"

# GitHub OAuth Configuration (placeholder values)
github_client_id = "your_github_client_id"
github_client_secret = "your_github_client_secret"
github_token = "your_github_token"

# Google Cloud Configuration
gcp_project_id = "your_gcp_project_id"
gcp_region = "us-west1"
gcp_zone = "us-west1-a"

# Oracle Cloud Configuration
tenancy_ocid = "ocid1.tenancy.oc1..your_tenancy_ocid"
user_ocid = "ocid1.user.oc1..your_user_ocid"
fingerprint = "your_oci_fingerprint"
private_key_path = "/app/oci-private-key.pem"
compartment_id = "ocid1.compartment.oc1..your_compartment_id"
oci_region = "us-sanjose-1"
oci_vcn_id = "ocid1.vcn.oc1.us-sanjose-1.your_vcn_id"

# IBM Cloud Configuration
ibmcloud_api_key = "your_ibm_cloud_api_key"
ibmcloud_region = "us-south"
ibm_security_group_id = "your_ibm_security_group_id"
resource_group_id = "your_ibm_resource_group_id"

# SSH Configuration
ssh_public_key = "ssh-rsa your_ssh_public_key_content"